sgjicq4i:Nov2Novelty a picr:F2M_Mapping ;
    picr:fieldOfMapping sgjicq4i:Nov ;
    picr:metricOfMapping papeval:Novelty .

sgjicq4i:Rel2Relevance a picr:F2M_Mapping ;
    picr:fieldOfMapping sgjicq4i:Rel ;
    picr:metricOfMapping papeval:Relevance .

sgjicq4i:Tc2TechnicalQuality a picr:F2M_Mapping ;
    picr:fieldOfMapping sgjicq4i:Tc ;
    picr:metricOfMapping papeval:TechnicalQuality .

sgjicq4i:Nov a picr:ReviewFormCriterion ;
    rdfs:label "nov" ;
    picr:fieldOf sgjicq4i: ;
    picr:maxVal 10 ;
    picr:minVal 5 ;
    picr:weight 2 .

sgjicq4i:Rel a picr:ReviewFormCriterion ;
    rdfs:label "rel" ;
    picr:fieldOf sgjicq4i: ;
    picr:maxVal 5 ;
    picr:minVal 0 ;
    picr:weight 1 .

sgjicq4i:Tc a picr:ReviewFormCriterion ;
    rdfs:label "tc" ;
    picr:fieldOf sgjicq4i: ;
    picr:maxVal 5 ;
    picr:minVal 0 ;
    picr:weight 1 .

sgjicq4i: a picr:ReviewForm .

